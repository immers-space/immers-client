{
  "presets": [[
    "@babel/preset-env",
    {
      "targets": ["defaults", "not ie 11", "node 12"], // node 12 = support importing with webpack 4
      "exclude": ["web.url"], // saves 30Kb in minified bundle
      "bugfixes": true,
      "useBuiltIns": "usage",
      "corejs": "3.17",
      "shippedProposals": true,
      "modules": false,
      "debug": true
    }
  ]]
}
